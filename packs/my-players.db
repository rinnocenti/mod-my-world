5500000},{"_id":"FHtxOqT1svmRxHBi","flags":{},"name":"Bedroll","type":"loot","img":"/systems/dnd5e/icons/items/inventory/cloth-blue.jpg","data":{"description":{"value":"<div>\n\t\t\t<div><i>Adventuring gear</i></div>\n\t\t\t<div class=\"row\">\n\t\t\t\t<div class=\"col-4\">1 gp, 7 lb.</div>\n\t\t\t\t<div class=\"col-8 text-right\">  </div>\n\t\t\t</div>\n\t\t</div>\n\t\t<hr>\n\t\t","chat":"","unidentified":""},"source":"PHB","quantity":1,"weight":7,"price":1,"attuned":false,"equipped":true,"rarity":"None","identified":false,"damage":{"parts":[]},"attributes":{"spelldc":10},"consume":{"type":"","target":"","amount":null}},"sort":5600000},{"_id":"Q7v23KZnK42g8XIg","flags":{},"name":"Mess Kit","type":"loot","img":"/systems/dnd5e/icons/items/inventory/fork.jpg","data":{"description":{"value":"<div>\n\t\t\t<div><i>Adventuring gear</i></div>\n\t\t\t<div class=\"row\">\n\t\t\t\t<div class=\"col-4\">2 sp, 1 lb.</div>\n\t\t\t\t<div class=\"col-8 text-right\">  </div>\n\t\t\t</div>\n\t\t</div>\n\t\t<hr>\n\t\t<div  class=\"rd__b  rd__b--2\"><p>This tin box contains a cup and simple cutlery. The box clamps together, and one side can be used as a cooking pan and the other as a plate or shallow bowl.</p></div>","chat":"","unidentified":""},"source":"PHB","quantity":1,"weight":1,"price":0.2,"attuned":false,"equipped":true,"rarity":"None","identified":false,"damage":{"parts":[]},"attributes":{"spelldc":10},"consume":{"type":"","target":"","amount":null}},"sort":5700000},{"_id":"7BF3AzO6cfdZz3Ka","flags":{},"name":"Tinderbox","type":"loot","img":"/systems/dnd5e/icons/items/inventory/torch.jpg","data":{"description":{"value":"<div>\n\t\t\t<div><i>Adventuring gear</i></div>\n\t\t\t<div class=\"row\">\n\t\t\t\t<div class=\"col-4\">5 sp, 1 lb.</div>\n\t\t\t\t<div class=\"col-8 text-right\">  </div>\n\t\t\t</div>\n\t\t</div>\n\t\t<hr>\n\t\t<div  class=\"rd__b  rd__b--2\"><p>This small container holds flint, fire steel, and tinder (usually dry cloth soaked in light oil) used to kindle a fire. Using it to light a torch - or anything else with abundant, exposed fuel - takes an action. Lighting any other fire takes 1 minute.</p></div>","chat":"","unidentified":""},"source":"PHB","quantity":1,"weight":1,"price":0.5,"attuned":false,"equipped":true,"rarity":"None","identified":false,"damage":{"parts":[]},"attributes":{"spelldc":10},"consume":{"type":"","target":"","amount":null}},"sort":5800000},{"_id":"PreVTlF67vqoYGyK","flags":{},"name":"Torch","type":"consumable","img":"/systems/dnd5e/icons/items/inventory/torch.jpg","data":{"description":{"value":"<div>\n\t\t\t<div><i>Adventuring gear</i></div>\n\t\t\t<div class=\"row\">\n\t\t\t\t<div class=\"col-4\">1 cp, 1 lb.</div>\n\t\t\t\t<div class=\"col-8 text-right\">  </div>\n\t\t\t</div>\n\t\t</div>\n\t\t<hr>\n\t\t<div  class=\"rd__b  rd__b--2\"><p>A <i>torch</i> burns for 1 hour, providing bright light in a 20-foot radius and dim light for an additional 20 feet. If you make a melee attack with a burning <i>torch</i> and hit, it deals 1 fire damage.</p></div>","chat":"","unidentified":""},"source":"PHB","quantity":9,"weight":1,"price":0.01,"attuned":false,"equipped":true,"rarity":"None","identified":false,"activation":{"type":"action","cost":1,"condition":""},"duration":{"value":1,"units":"hour"},"target":{"value":40,"units":"ft","type":"radius"},"range":{"value":null,"long":null,"units":""},"uses":{"value":1,"max":1,"per":"charges","autoDestroy":false,"autoUse":true},"consume":{"type":"","target":"","amount":null,"value":"","_deprecated":true},"ability":"str","actionType":"mwak","attackBonus":0,"chatFlavor":"","critical":null,"damage":{"parts":[["1","fire"]],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"consumableType":"trinket","charges":{"value":1,"max":1,"_deprecated":true},"autoUse":{"value":true,"_deprecated":true},"autoDestroy":{"value":true,"_deprecated":true},"attributes":{"spelldc":10}},"sort":6300000},{"_id":"4odfrDR4e7u9Ijw0","flags":{},"name":"Rations (1 day)","